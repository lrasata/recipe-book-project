<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity IngredientOrder.
    -->
    <changeSet id="20220120035817-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="ingredient_id"
                                 baseTableName="ingredient_order"
                                 constraintName="fk_ingredient_order__ingredient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ingredient"/>

        <addForeignKeyConstraint baseColumnNames="shopping_list_id"
                                 baseTableName="ingredient_order"
                                 constraintName="fk_ingredient_order__shopping_list_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shopping_list"/>
    </changeSet>
</databaseChangeLog>
